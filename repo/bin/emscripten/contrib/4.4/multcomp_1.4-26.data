/* Warpbreaks example - Bonferroni adjustment */
proc glimmix data = warpbreaks;
	class tension;
	model breaks = tension;
	estimate 'M - L' tension 0 -1  1,
    	     'H - L' tension 1 -1  0,
        	 'H - M' tension 1  0 -1 / adjust = bon;
run;

