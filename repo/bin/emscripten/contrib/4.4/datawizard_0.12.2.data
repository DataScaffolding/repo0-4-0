<span id="cb6-37"><a href="#cb6-37" tabindex="-1"></a><span class="co">#&gt; 9           5.8         2.7  virginica</span></span>
<span id="cb6-38"><a href="#cb6-38" tabindex="-1"></a><span class="co">#&gt; 10          6.1         3.0 versicolor</span></span>
<span id="cb6-39"><a href="#cb6-39" tabindex="-1"></a></span>
<span id="cb6-40"><a href="#cb6-40" tabindex="-1"></a><span class="fu">data_select</span>(iris, <span class="fu">regex</span>(<span class="st">&quot;^Sep|ies&quot;</span>))</span>
<span id="cb6-41"><a href="#cb6-41" tabindex="-1"></a><span class="co">#&gt;    Sepal.Length Sepal.Width    Species</span></span>
